{
	"name": "cbfc-watch",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"engines": {
		"node": ">=20.9.0",
		"pnpm": ">=8.7.1"
	},
	"packageManager": "pnpm@8.7.1",
	"scripts": {
		"dev": "vite dev --host",
		"build": "if [ \"$NODE_ENV\" != \"development\" ]; then rm -rf src/routes/api/og; fi && NODE_VERSION=20.9.0 CF_PAGES=1 vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"update-data": "python3 scripts/data_import.py --fetch",
		"update-data-remote": "python3 scripts/data_import.py --fetch --db-mode remote",
		"index-search": "set -a && [ -f .env ] && source .env && set +a && ./scripts/search_sync.py src/lib/data/data.csv \"$PUBLIC_TYPESENSE_PROTOCOL\" \"$PUBLIC_TYPESENSE_HOST\" \"$TYPESENSE_ADMIN_API_KEY\"",
		"build-notebook": "cd analysis && R -e \"rmarkdown::render('cbfc-analysis.rmd')\" && mv cbfc-analysis.html ../static/notebook/index.html && cp *.json ../src/lib/data/charts/ 2>/dev/null || true",
		"clean": "rm -rf .svelte-kit build node_modules/.vite"
	},
	"devDependencies": {
		"@cloudflare/workers-types": "^4.20250517.0",
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@exodus/schemasafe": "^1.3.0",
		"@fontsource-variable/archivo": "^5.2.5",
		"@iconify/svelte": "^4.2.0",
		"@lucide/svelte": "^0.482.0",
		"@rollup/plugin-dsv": "^3.0.5",
		"@sinclair/typebox": "^0.34.33",
		"@sveltejs/adapter-cloudflare": "^7.0.3",
		"@sveltejs/adapter-static": "^3.0.8",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/container-queries": "^0.1.1",
		"@tailwindcss/vite": "^4.1.8",
		"@types/node": "^20.9.0",
		"@typeschema/class-validator": "^0.2.0",
		"@vinejs/vine": "^1.8.0",
		"arktype": "^2.1.20",
		"bits-ui": "1.8.0",
		"class-validator": "^0.14.2",
		"clsx": "^2.1.1",
		"cmdk-sv": "^0.0.19",
		"csv-parse": "^5.6.0",
		"csv-parser": "^3.2.0",
		"d3": "^7.9.0",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^3.0.0",
		"extract-colors": "^4.2.0",
		"formsnap": "2.0.0-next.1",
		"globals": "^16.0.0",
		"joi": "^17.13.3",
		"lucide-svelte": "^0.487.0",
		"mode-watcher": "^1.0.7",
		"paneforge": "1.0.0-next.5",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.12",
		"superstruct": "^2.0.2",
		"svelte": "^5.33.18",
		"svelte-check": "^4.0.0",
		"svelte-headless-table": "^0.18.3",
		"svelte-sonner": "^0.3.28",
		"sveltekit-superforms": "^2.26.1",
		"tailwind-merge": "^3.3.0",
		"tailwind-variants": "^0.2.1",
		"tailwindcss": "^4.1.8",
		"tw-animate-css": "^1.3.4",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"valibot": "^0.42.1",
		"vaul-svelte": "1.0.0-next.7",
		"vite": "^6.2.5",
		"vite-plugin-wasm-module-workers": "^0.0.2",
		"yup": "^1.6.1",
		"zod": "^3.25.56"
	},
	"pnpm": {
		"onlyBuiltDependencies": [
			"esbuild"
		]
	},
	"dependencies": {
		"@resvg/resvg-js": "^2.6.2",
		"@resvg/resvg-wasm": "^2.6.2",
		"d3-hierarchy": "^3.1.2",
		"d3-sankey": "^0.12.3",
		"flexsearch": "^0.8.158",
		"instantsearch.js": "^4.78.3",
		"pack-spheres": "^1.0.4",
		"satori": "^0.18.2",
		"satori-html": "^0.3.2",
		"sharp": "^0.34.3",
		"svelteplot": "^0.4.1",
		"tailwindcss-animate": "^1.0.7",
		"typesense": "^2.0.3",
		"typesense-instantsearch-adapter": "^2.9.0",
		"workers-og": "^0.0.27"
	},
	"meta": {
		"analyticsID": "cbfc.watch"
	}
}
